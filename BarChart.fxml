<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="690.0" prefWidth="892.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.BarChartController">
   <children>
      <LineChart fx:id="lineChart" layoutX="33.0" layoutY="29.0" prefHeight="387.0" prefWidth="832.0" title="Expense chart">
        <xAxis>
          <CategoryAxis label="Expenses" prefHeight="32.0" prefWidth="420.0" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Dollars" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="loadLineChartButton" layoutX="464.0" layoutY="481.0" mnemonicParsing="false" onAction="#addToListOnClick" prefHeight="25.0" prefWidth="148.0" text="Add To List" />
   <TextField fx:id="newItemName" layoutX="60.0" layoutY="481.0" promptText="Enter Expense Name" />
      <TextField fx:id="newItemValue" layoutX="262.0" layoutY="481.0" promptText="Amount" />
      <Button fx:id="back" layoutX="60.0" layoutY="608.0" mnemonicParsing="false" onAction="#goBackButtonOnClick" text="Go Back" />
      <Button fx:id="loadLineChartButton1" layoutX="621.0" layoutY="481.0" mnemonicParsing="false" onAction="#loadLineChartButtonOnClick" prefHeight="25.0" prefWidth="148.0" text="Load Line Chart" />
   </children>
</AnchorPane>
